@model List<WebOperaciones.ConductorWS.Conductor>
<div id="IndexPartialDiv">
    <table class="table table-sm table-bordered table-striped table-hover" style="margin-top: 40px;">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">DNI</th>
                <th scope="col">NOMBRES</th>
                <th scope="col">APELLIDOS</th>
                <th scope="col">FECHA INGRESO</th>
                <th scope="col">CATEGORIA</th>
                <th scope="col">LICENCIA</th>
                <th scope="col">ESTADO</th>
                <th scope="col">DIRECCIÓN</th>
                <th colspan="2">&nbsp;</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td scope="row">@Html.DisplayFor(modelItem => item.dni_conductor)</td>
                    <td>@Html.DisplayFor(modelItem => item.dni_conductor)</td>
                    <td>@Html.DisplayFor(modelItem => item.des_nombres)</td>
                    <td>@Html.DisplayFor(modelItem => item.des_apellido)</td>
                    <td>@Html.DisplayFor(modelItem => item.fecha_ingreso)</td>
                    <td>@Html.DisplayFor(modelItem => item.categoria_licencia)</td>
                    <td>@Html.DisplayFor(modelItem => item.nro_licencia)</td>
                    <td>@Html.DisplayFor(modelItem => item.tx_estado)</td>
                    <td>@Html.DisplayFor(modelItem => item.direccion)</td>
                    <td>@Html.ActionLink("Editar", "Edit", "Conductor", new { id = item.dni_conductor }, new { @class = "btn btn-outline-primary btn-sm" })</td>
                    <td>@Html.ActionLink("Eliminar", "Delete", "Conductor", new { id = item.dni_conductor }, new { @class = "btn btn-outline-danger btn-sm" })</td>
                </tr>
            }
        </tbody>
    </table>
</div>
