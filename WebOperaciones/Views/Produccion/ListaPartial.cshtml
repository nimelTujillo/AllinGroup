@model List<WebOperaciones.Dominio.Produccion>
<div id="IndexPartialDiv">
    <table class="table table-sm table-bordered table-striped table-hover" style="margin-top: 40px;">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">DNI CONDUCTOR</th>
                <th scope="col">BUS</th>
                <th scope="col">INGRESO</th>
                <th scope="col">FECHA</th>
                <th colspan="2">&nbsp;</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td scope="row">@Html.DisplayFor(modelItem => item.Id_produccion)</td>
                    <td>@Html.DisplayFor(modelItem => item.Dni_conductor)</td>
                    <td>@Html.DisplayFor(modelItem => item.Id_bus)</td>
                    <td>@Html.DisplayFor(modelItem => item.Can_ingreso)</td>
                    <td>@Html.DisplayFor(modelItem => item.Fecha)</td>
                    
                    <td>@Html.ActionLink("Eliminar", "Delete", "Produccion", new { id = item.Id_produccion }, new { @class = "btn btn-outline-danger btn-sm" })</td>
                </tr>
            }
        </tbody>
    </table>
</div>
